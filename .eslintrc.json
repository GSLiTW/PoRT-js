{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": "off",
        "valid-jsdoc": "off",
        "new-cap": "off",
        "guard-for-in": "off",
        // "require-jsdoc": "off",
        "camelcase": "off"
    }
}
